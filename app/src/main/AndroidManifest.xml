<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.necej.necej_cp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/necej_fg"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".JogoActivity"
            android:theme="@style/AppTheme"
            android:parentActivityName=".TelaInicialActivity"
            android:screenOrientation="portrait">

        </activity>
        <activity android:name=".SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TelaInicialActivity"
            android:label="@string/actbar_inicial"></activity>
    </application>

</manifest>